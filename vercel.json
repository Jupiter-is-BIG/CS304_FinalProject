{
    "functions": {
        "api/**/*.php": { "runtime": "vercel-php@0.6.0" }
    
    },
    "routes": [
        { "src": "/(.+.php)", "dest": "/$1" },
        { "src": "/img/(.*)", "dest": "/img/$1" },
        { "src": "/ddl/(.*)", "dest": "/ddl/$1" },
        { "src": "/(.*)", "dest": "/api/index.php" }
    ]
}
